  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Java Launch File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "name": "Go Launch File",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${file}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Launch current file",
      "program": "${workspaceRoot}/target/debug/${fileBasenameNoExtension}",
      "args": [],
      "cwd": "${workspaceRoot}",
      "sourceLanguages": ["rust"]
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'httpfs'",
      "cargo": {
        "args": ["test", "--no-run", "--lib", "--package=httpfs"],
        "filter": {
          "name": "httpfs",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
